package com.morphgame.morphgame;

public class stats {
	private int score;
	private String name;
	
	public stats(String n,int s){
		score = s;
		name = n;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getScore() {
		return score;
	}
	public void setScore(int score) {
		this.score = score;
	}
	
	
}
